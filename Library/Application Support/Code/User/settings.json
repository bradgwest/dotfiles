{
    "[go]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        },
        "editor.formatOnSave": true,
        "editor.insertSpaces": false
    },
    "[groovy]": {
        "editor.tabSize": 2
    },
    "[json]": {
        "editor.tabSize": 4,
        "editor.wordWrap": "on"
    },
    "[markdown]": {
        "editor.quickSuggestions": {
            "comments": "off",
            "other": "off",
            "strings": "off"
        },
        "editor.wordWrap": "on"
    },
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        },
        "editor.defaultFormatter": "ms-python.python",
        "editor.formatOnSave": true,
        "editor.semanticHighlighting.enabled": false,
        "editor.tabSize": 4
    },
    "[terraform]": {
        "editor.formatOnSave": true
    },
    "[yaml]": {
        "editor.autoIndent": "keep",
        "editor.insertSpaces": true,
        "editor.quickSuggestions": {
            "comments": false,
            "other": false,
            "strings": false
        },
        "editor.tabSize": 2,
    },
    "breadcrumbs.enabled": true,
    "cSpell.enableFiletypes": [
        "sql"
    ],
    "cSpell.ignoreWords": [
        "dedup",
        "kubernetes",
        "detokenize",
        "expn",
        "detokenizing",
        "detokenized",
        "multitenant",
        "uncordon",
        "Rollouts",
        "backoff"
    ],
    "comments.openView": "never",
    "debug.internalConsoleOptions": "neverOpen",
    "debug.showBreakpointsInOverviewRuler": true,
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.cursorStyle": "line",
    "editor.detectIndentation": false,
    "editor.fontFamily": "'Source Code Pro', Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 12,
    "editor.formatOnSave": false,
    "editor.guides.bracketPairs": true,
    "editor.inlineSuggest.enabled": true,
    "editor.insertSpaces": true,
    "editor.lineNumbers": "on",
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.parameterHints.enabled": false,
    "editor.quickSuggestions": {
        "comments": false,
        "other": true,
        "strings": false
    },
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [
        80,
        88,
        120
    ],
    "editor.suggestSelection": "first",
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "editor.wordWrap": "off",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "[Dd]ockerfile*": "dockerfile",
        "docker-compose*": "yaml"
    },
    "files.exclude": {
        "**/*.pyc": true,
        "**/._*": true
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.mergeEditor": false,
    "go.autocompleteUnimportedPackages": true,
    "go.docsTool": "godoc",
    "go.formatFlags": [
        "-local"
    ],
    "go.formatTool": "goimports",
    "go.languageServerExperimentalFeatures": {
        "diagnostics": true,
        "documentLink": true,
        "format": true
    },
    "go.testFlags": [],
    "gopls": {
        "completeUnimported": true,
        "deepCompletion": true,
        "usePlaceholders": true
    },
    "html.format.extraLiners": "body",
    "html.format.indentInnerHtml": true,
    "html.format.unformatted": "b,em,i,span,strong,wbr",
    "html.format.wrapLineLength": 0,
    "markdown.extension.toc.levels": "2..6",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "openInGitHub.defaultBranch": "master",
    "openInGitHub.defaultRemote": "origin",
    "openInGithub.maxBuffer": 512000,
    "openInGitHub.excludeCurrentRevision": false,
    "plantuml.java": "/Users/bw/.asdf/shims/java",
    "python.formatting.provider": "autopep8",
    "python.linting.enabled": true,
    "python.venvFolders": [
        ".venv",
        "/Users/bw/src/dev-tools/scripts/k8s_update_version/.venv"
    ],
    "search.exclude": {
        "**/*.code-search": true,
        "**/.eggs": true,
        "**/.idea": true,
        "**/.venv": true,
        "**/bower_components": true,
        "**/data_eng.egg-info": true,
        "**/node_modules": true,
        ".coverage": true,
        "coverage.xml": true
    },
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.cursorStyle": "underline",
    "vim.cursorStylePerMode.insert": "underline",
    "vim.cursorStylePerMode.normal": "block",
    "vim.cursorStylePerMode.replace": "",
    "vim.cursorStylePerMode.visual": "line",
    "vim.easymotion": true,
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false
    },
    "vim.hlsearch": true,
    "vim.incsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "after": [
                "<Esc>"
            ],
            "before": [
                "j",
                "j"
            ]
        }
    ],
    "vim.leader": "<space>",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "after": [
                "d",
                "d"
            ],
            "before": [
                "<leader>",
                "d"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":nohl"
            ]
        }
    ],
    "vim.sneak": true,
    "vim.useCtrlKeys": true,
    "vim.useSystemClipboard": true,
    "workbench.colorCustomizations": {
        "editorCursor.foreground": "#ff00f2",
        "editorGutter.background": "#9b9b9b85",
        "editorIndentGuide.activeBackground": "#FFA500",
        "editorLineNumber.foreground": "#000000",
        "statusBar.background": "#B48EAD",
        "statusBar.debuggingBackground": "#B48EAD",
        "statusBar.foreground": "#000000",
        "statusBar.noFolderBackground": "#B48EAD"
    },
    "workbench.colorTheme": "Solarized Light",
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.showTabs": true,
    "workbench.editor.tabSizing": "shrink",
    "workbench.editor.untitled.hint": "hidden",
    "workbench.editorAssociations": {
        "filenamePattern": "*.ipynb",
        "viewType": "jupyter.notebook.ipynb"
    },
    "workbench.panel.opensMaximized": "never",
    "workbench.startupEditor": "none",
    "yaml.schemas": {
        "https://github.com/SchemaStore/schemastore/blob/master/src/schemas/json/github-workflow.json": "/.github/workflows/",
    },
    "redhat.telemetry.enabled": false,
    "github.copilot.enable": {
        "*": true,
        "yaml": true,
        "plaintext": false,
        "markdown": true
    }
}
